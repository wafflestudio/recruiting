<%= form_for @applicant do |f| %>
	<div class="form_container">
		<div class="applicant_form">
			<div class="field">
				<%= f.label :name %>
				<%= f.text_field :name %>
			</div>

			<div class="field">
				<%= f.label :email %>
				<%= f.text_field :name %>
			</div>

			<div class="field">
				<%= f.label :password %>
				<%= f.password_field :password %>
			</div>

			<div class="field">
				<%= f.label :password_confirmation %>
				<%= f.password_field :password_confirmation %>
			</div>
		</div>

		<div class="questions_form">
			<div class="field">
				<% f.label %>
			</div>


		</div>


	</div>


<% end %>

